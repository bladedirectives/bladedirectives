{
    "name": "bladedirectives/bladedirectives",
    "description": "A Collection of Laravel Blade Directives Package.",
    "license": "MIT",
    "homepage": "https://bladedirectives.github.io/",
    "authors": [
        {
            "name": "Deyaa Muhammad",
            "email": "mail@deyaa.me",
            "homepage": "https://deyaa.me"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "bladeDirectives\\bladeDirectives\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeDirectives\\BladeDirectives\\BladeDirectivesServiceProvider"
            ],
            "aliases": {
              "BladeDirectives" : "BladeDirectives\\BladeDirectives\\BladeDirectivesFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
